@{ var usuario = (User)Model;     }<form action="/user/registrar" class="col-6" method="post">    <br />    <strong>Username</strong>    <div class="form-group" id="user-group">        <input type="text" class="form-control" placeholder="Nombre de Usuario" name="Username" />        @Html.ValidationMessage("Username")    </div>    <strong>Contraseña</strong>    <div class="form-group" id="contrasena-group">        <input type="password" class="form-control" placeholder="Contraseña" name="password" />        @Html.ValidationMessage("password")    </div>    <strong>Confirme su Contraseña</strong>    <div class="form-group" id="contrasena-group">        <input type="password" class="form-control" placeholder="Confirmar Contraseña" name="passwordConf" />        @Html.ValidationMessage("ConfPassword")    </div>    <button type="submit" class="btn btn-danger"><i class="fas fa-sign-in-alt"></i>Crear Usuario</button></form>