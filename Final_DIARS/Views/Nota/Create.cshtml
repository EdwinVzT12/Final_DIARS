@{ Layout = null;    var nota = (Nota)Model;}<form id="createNota" onsubmit="guardar(event)" action="/Nota/Create" enctype="multipart/form-data" method="post">    <div class="form-group">        <b>            <label>Titulo</label>        </b>        <input type="text" class="form-control" name="Titulo" value="@Model.Titulo" />        @Html.ValidationMessage("Titulo")    </div>    <div class="form-group">        <b>            <label>Contenido</label>        </b>        <textarea id="message" class="form-control" name="Contenido" rows="4" cols="50"></textarea>        @Html.ValidationMessage("Contenido")    </div>    <div class="form-group">        <b>            <label>Etiquetas</label>        </b>        @foreach (var item in ViewBag.Etiquetas)        {<div class="form-check form-check">    <input name="etiqueta" class="form-check-input" type="checkbox" id="inlineCheckbox1" value="@item.Id">    <label class="form-check-label" for="inlineCheckbox1">@item.Nombre</label></div>}        @Html.ValidationMessage("etiqueta")    </div></form>